  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "git stash -k -u && yarn translate && git add src/i18n/messages && git stash pop && lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "yarn lint",
      "git add"
    ]
  }
