    "@vue/test-utils": "1.0.0-beta.25",
    "autoprefixer": "^9.7.4",
    "axios": "0.18.0",
    "axios-mock-adapter": "^1.17.0",
    "core-js": "^3.5.0",
    "dayjs": "^1.8.17",
    "debounce-promise": "^3.1.2",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "portal-vue": "^2.1.6",
